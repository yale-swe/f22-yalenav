    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/yalenav-favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9d08665a-07fb-4681-89bf-97277032649b"
      }
    }
  }
}
